<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/"
  xmlns:cber="http://temenos.com/CBEREMITANCE"
  xmlns:fun="http://temenos.com/FUNDSTRANSFERCBEREMITANCE">
  <soapenv:Header/>
  <soapenv:Body>
    <cber:RMTFundtransfer>
      <WebRequestCommon>
        <!--Optional:-->
        <company/>
        <password>${password}</password>
        <userName>${username}</userName>
      </WebRequestCommon>
      <OfsFunction>
      </OfsFunction>
      <FUNDSTRANSFERCBEREMITANCEType id="">
        <fun:DEBITAMOUNT>${debitAmount}</fun:DEBITAMOUNT>
        <!--Optional:-->
        <fun:DEBITTHEIRREF>${debitTheIrRef}</fun:DEBITTHEIRREF>
        <!--Optional:-->
        <fun:CREDITTHEIRREF>${creditTheIrRef}</fun:CREDITTHEIRREF>
        <!--Optional:-->
        <fun:CREDITACCTNO>${creditAcctNo}</fun:CREDITACCTNO>
        <!--Optional:-->
        <fun:CREDITCURRENCY>${creditCurrency}</fun:CREDITCURRENCY>
        <!--Optional:-->
        <fun:CREDITAMOUNT></fun:CREDITAMOUNT>
        <!--Optional:-->
        <fun:CREDITVALUEDATE></fun:CREDITVALUEDATE>
        <!--Optional:-->
        <fun:RemitterName>${remitterName}</fun:RemitterName>
        <!--Optional:-->
        <fun:BeneficiaryName>${beneficiaryName}</fun:BeneficiaryName>
        <!--Optional:-->
        <fun:BENCUST></fun:BENCUST>
        <!--Optional:-->
        <fun:ORDCUST></fun:ORDCUST>
      </FUNDSTRANSFERCBEREMITANCEType>
    </cber:RMTFundtransfer>
  </soapenv:Body>
</soapenv:Envelope>
