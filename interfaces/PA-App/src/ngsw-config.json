{
  "$schema": "../node_modules/@angular/service-worker/config/schema.json",
  "index": "index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/index.html", "/**.css", "/**.js", "/manifest.json"]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/*", "/assets/(**|!i18n)/*", "/svg/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": [
        "**/instance",
        "**/programs/published/all",
        "**/programs/*"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 200,
        "maxAge": "3d"
      }
    }
  ]
}
